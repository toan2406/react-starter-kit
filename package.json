{
  "name": "react-starter-kit",
  "version": "1.0.0",
  "description": "React starter kit",
  "repository": "toan2406/react-starter-kit",
  "main": "index.js",
  "scripts": {
    "dev": "BABEL_ENV=server babel-watch src/server & webpack --config webpack.dev.js --watch",
    "build:client": "webpack --config webpack.prod.js",
    "build:server": "BABEL_ENV=server babel src --out-dir build --ignore client,**/*.test.js --copy-files",
    "build": "npm run build:server && npm run build:client",
    "start": "node build/server",
    "test": "flow && jest"
  },
